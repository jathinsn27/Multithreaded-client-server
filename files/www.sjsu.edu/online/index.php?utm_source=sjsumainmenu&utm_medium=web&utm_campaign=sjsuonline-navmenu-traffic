<script>

		
	/* Basic JavaScript to power the Apply Now modal dialog  */
	const showButton = document.getElementById("showModal");
	const showButtonMobile = document.getElementById("mobileShowModal");
	const applyModal = document.getElementById("applyModal");
	const applyModalMobile = document.getElementById("mobileApplyModal");
	const closeButton = document.getElementById("closeModal");
	const closeButtonMobile = document.getElementById("mobileCloseModal");
	showButton.addEventListener("click", () => {
		console.log("Event listener for:" + showButton + "successfully fired");
		applyModal.showModal();
	});
	showButtonMobile.addEventListener("click", () => {
		console.log("Event listener for:" + showButtonMobile + "successfully fired");
		mobileApplyModal.showModal();
	});	

	
	
</script>


