                  <div class="grid-container chapter-break-container">
                     
                     <h2 class="chapter-title">Why SJSU Online?</h2>
                     </div>
                  
                  <section class="differentiators component component--tsp global-spacing global-spacing--4xlarge">
                     <div class="differentiators__lower global-spacing global-spacing--xlarge">
                        <div class="grid-container">
                           <div class="grid-x grid-margin-x grid-margin-y">
                              
                              	
                              <div class="initial-12 medium-4 cell">
                                 		
                                 <div class="card card--differentiator ">
                                    			
                                    <div class="card__content">
                                       				
                                       <div class="card__content__inner">
                                          					
                                          <div class="card__content__graphic">
                                             						
                                             <div class="icon">
                                                <img src="https://newdev.sjsu.edu/online/_resources/img/sjsuo-icon_flex.png" alt="Person on laptop icon" aria-hidden="true">
